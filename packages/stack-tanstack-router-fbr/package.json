{
  "name": "@router-learning/stack-tanstack-router-fbr",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "generate": "openapi-typescript ../../openapi.yaml -o src/generated/api.ts",
    "test": "vitest run",
    "lint": "biome check .",
    "lint:fix": "biome check --write",
    "lint:fix:unsafe": "biome check --write --unsafe",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-router": "^1.121.34",
    "@tanstack/router-vite-plugin": "^1.121.34",
    "openapi-fetch": "^0.14.0",
    "openapi-react-query": "^0.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-router-devtools": "^1.121.34",
    "@tanstack/router-plugin": "^1.121.34",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "openapi-typescript": "^7.8.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
